{
"sourceType": "unambiguous",
env: {
        development: {
            sourceMaps: 'inline',
            plugins: ['source-map-support']
        }
    },
            "presets": [
              [
                "@babel/preset-env",
                { "targets": "defaults", "modules": "commonjs" },
              ],
            ],
            "plugins": [
              [
                "@babel/plugin-transform-runtime",
                { "regenerator": true, "corejs": 3, "useESModules": false },
              ],
              "@babel/plugin-proposal-class-properties",
        
            ],
          }

{
   "env":{
      "es2021":true,
      "node":true
   },
   "plugins":[
      "import",
      "node",
      "promise",
      "jsdoc"
   ],
   "globals":{
      "document":"readonly",
      "navigator":"readonly",
      "window":"readonly"
   },
   "extends":[
      "plugin:jsdoc/recommended",
      "eslint:recommended"
   ],
   "rules":{
      "no-unused-vars":"off"
   },
   "parser":"@babel/eslint-parser",
   "parserOptions":{
      "sourceType":"module",
      "allowImportExportEverywhere":false,
      "ecmaFeatures":{
         "globalReturn":true
      },
         "requireConfigFile": false
   }
}

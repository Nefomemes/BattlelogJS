{
  "name": "battlelog.js",
  "version": "0.2.8",
  "description": "[WIP] Unofficial API wrapper for battlelog.battlefield.com",
  "main": "dist/bundle.prod.min.js",
  "scripts": {
    "test": "jest",
    "gitconfig": "bash gitconfig.sh",
    "lint": "eslint ./src --fix",
    "lint:report": "eslint --output-file eslint_report.json --format json ./src",
    "docs": "docgen --source ./src --output dist/docs.json",
    "docs:test": "npx docgen --source ./src",
    "build:prod": "webpack --mode production --output-filename bundle.prod.min.js",
    "build:dev": "npx webpack --mode development --output-filename bundle.dev.js ",
    "build": "yarn build:prod ; yarn build:dev"
  },
  "keywords": [
    "battlelog",
    "battlefield"
  ],
  "author": "Nefomemes",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.13.10",
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@babel/core": ">=7.2.0",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.12.16",
    "@babel/runtime-corejs3": "^7.13.10",
    "babel-loader": "^8.2.2",
    "discord.js-docgen": "git+https://github.com/Nefomemes/docgen.git",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.0.2",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^26.6.3",    
    "eslint-plugin-promise": "^5.1.0",
    "node-polyfill-webpack-plugin": "^1.0.3",
    "prettier": "2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.27.2",
    "webpack-cli": "^4.5.0"
  }
}
